<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<BorderPane onKeyPressed="#keySubmit" prefHeight="720.0" prefWidth="1080.0" style="-fx-background-color: WHITE;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.AnyadirSocioController">
   <top>
      <BorderPane prefHeight="40.0" style="-fx-background-color: #4DB6AC;" BorderPane.alignment="CENTER">
         <left>
            <HBox BorderPane.alignment="CENTER">
               <children>
               	<JFXButton onMouseClicked="#goBack" prefHeight="32.0" prefWidth="32.0" text=" ">
               	   <graphic>
                        <ImageView fitHeight="32.0" fitWidth="32.0">
                           <image>
                              <Image url="@../../resources/arrow_back.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </JFXButton>
               </children>
            </HBox>
         </left>
         <right>
            <HBox alignment="CENTER_RIGHT" spacing="10.0" style="-fx-background-color: #4DB6AC;" BorderPane.alignment="TOP_RIGHT">
         <children>
            <JFXButton buttonType="RAISED" onMouseClicked="#buttonMiArea" prefHeight="30.0" prefWidth="120.0" style="-fx-background-color: #263238;" text="Mi área" textFill="WHITE">
            <font>
                  <Font size="14.0" />
               </font>
               </JFXButton>
            <JFXButton buttonType="RAISED" onMouseClicked="#buttonSocios" prefHeight="30.0" prefWidth="120.0" style="-fx-background-color: #263238;" text="Socios" textFill="WHITE">
            <font>
                  <Font size="14.0" />
               </font>
               </JFXButton>
            <JFXButton buttonType="RAISED" onMouseClicked="#buttonPeliculas" prefHeight="30.0" prefWidth="120.0" style="-fx-background-color: #263238;" text="Películas" textFill="WHITE">
            <font>
                  <Font size="14.0" />
               </font>
               </JFXButton>
            <JFXButton buttonType="RAISED" onMouseClicked="#logOut" prefHeight="30.0" prefWidth="120.0" style="-fx-background-color: #263238;" text="Cerrar sesión" textFill="WHITE">
            <font>
                  <Font size="14.0" />
               </font>
               </JFXButton>
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </HBox>
         </right>
      </BorderPane>
   </top>
   <center>
      <VBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <VBox>
               <children>
            <Label text="Añadir socio">
                  <font>
                     <Font size="30.0" />
                  </font>
                  <padding>
                        <Insets left="10.0" />
               </padding>
               </Label>
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="5.0" VBox.vgrow="SOMETIMES">
               <children>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="DNI">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="dniField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Nombre" textAlignment="RIGHT">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="nameField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Dirección" textAlignment="RIGHT">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="dirField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Teléfono" textAlignment="RIGHT">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="tlfField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Email">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="emailField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Username">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="usernameField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label alignment="CENTER_RIGHT" focusTraversable="false" prefWidth="70.0" text="Contraseña">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="pwdField" prefWidth="175.0" style="-fx-background-color: #CFD8DC;">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </HBox>
                  <Text fx:id="error" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: rgb(213, 0, 0);" visible="false" />
                  <HBox alignment="CENTER" prefHeight="70.0" spacing="10.0">
                     <children>
                        <JFXButton onMouseClicked="#add" prefHeight="30.0" style="-fx-background-color: #4DB6AC;" text="Añadir">
                        <font>
                  <Font size="14.0" />
               </font></JFXButton>
               <JFXButton onMouseClicked="#goBack" prefHeight="30.0" style="-fx-background-color: #4DB6AC;" text="Cancelar">
                        <font>
                  <Font size="14.0" />
               </font></JFXButton>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
